@{
    ViewData["Title"] = "Home Page";
}
@model LocationViewModel

<div class="text-center">
    <h1 class="display-4">Nearest Fuel Stations</h1>
    <p>Search for nearby Fuel Stations below.</p>
    <form method="get">
        <select asp-for="Location" asp-items="Model.Locations"></select>
        <button asp-controller="Home" asp-action="Search">Search</button>

    </form>
</div>
